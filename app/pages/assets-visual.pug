extends extends/layout.pug

block html_head
	- var pageTitle = 'Logo assets and visual identity'
	- var pageDescription = 'Here you can download raster or vector logotype of Drone P2P framework for press purpose. Also get familiar with our visual identity.'
	- var pageKeywords = ''

block html_head_og
	- var ogUrl = 'assets.html'
	- var ogTitle = 'Logo assets and visual identity for Drone P2P'
	- var ogDescription = 'Here you can download raster or vector logotype of Drone P2P framework for press purpose. Also get familiar with our visual identity.'

block header
	- var header_banner = false

block html
	- var bodyclass = ''


block content
	main(role="main")
		.w-text
			section
				h1 Logo and other assets

				h2 Visual guide 2018
				
				p.theme-dark: a(href="https://github.com/drone-p2p/visual-identity", target="_blank").d-b
					img.i-block(src="assets/i/visual-guide-2018-dark@2x.png")

				p.theme-light: a(href="https://github.com/drone-p2p/visual-identity", target="_blank").d-b
						img.i-block(src="assets/i/visual-guide-2018-light@2x.png")
					
				p.t-align-center
					a.t-uppercase(href="https://github.com/drone-p2p/visual-identity", target="_blank") Download


				h2 Logo Drone-p2p

				.row
					.col-sm-6.back-dark.t-align-center
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-4-drone-p2p-logo-light.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/4-drone-p2p-logo-light.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/4-drone-p2p-logo-light.eps", target="_blank", download): b EPS
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-5-drone-p2p-logo-light-neon.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/5-drone-p2p-logo-light-neon.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/5-drone-p2p-logo-light-neon.eps", target="_blank", download): b EPS
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-6-drone-p2p-logo-light-blue.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/6-drone-p2p-logo-light-blue.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/6-drone-p2p-logo-light-blue.eps", target="_blank", download): b EPS
					.col-sm-6.back-light.t-align-center
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-1-drone-p2p-logo-dark.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/1-drone-p2p-logo-dark.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/1-drone-p2p-logo-dark.eps", target="_blank", download): b EPS
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-2-drone-p2p-logo-dark-blue-1.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/2-drone-p2p-logo-dark-blue-1.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/2-drone-p2p-logo-dark-blue-1.eps", target="_blank", download): b EPS
						.assets-logo
							img.i-block(alt="", src="assets/i/assets-logo/preview-3-drone-p2p-logo-dark-blue-2.png")
							span.assets-logo--download
								span.assets-logo--download-inside
									a(href="assets/i/assets-logo/files/3-drone-p2p-logo-dark-blue-2.png", target="_blank", download): b PNG
									a(href="assets/i/assets-logo/files/3-drone-p2p-logo-dark-blue-2.eps", target="_blank", download): b EPS

				br/
				p.t-align-center
					| Download logo by hovering or download whole pach in 
					a(href="assets/i/assets-logo/drone-p2p-logo-assets.zip", download) one .zip